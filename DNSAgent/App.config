<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.diagnostics>
    <sources>
      <source name="DNSAgent">
        <listeners>
          <add name="eventLogTraceListener" type="System.Diagnostics.EventLogTraceListener" initializeData="DNSAgent">
            <filter type="System.Diagnostics.EventTypeFilter" initializeData="Error"></filter>
          </add>
        </listeners>
      </source>
    </sources>

  </system.diagnostics>
</configuration>